{
  "name": "ng2-datetimepicker",
  "version": "0.0.3",
  "description": "Angular 2 datetime picker",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "copy:bs": "copyfiles -f node_modules/bootstrap/dist/css/bootstrap.min.css assets/css & copyfiles -f node_modules/bootstrap/dist/fonts/* assets/fonts",
