  "license": "MIT",
  "bugs": {
    "url": "https://github.com/connecttheworld/ng2-datetimepicker/issues"
  },
  "homepage": "https://github.com/connecttheworld/ng2-datetimepicker#readme",
  "dependencies": {
    "@angular/common": "^2.0.1",
    "@angular/compiler": "^2.0.1",
    "@angular/core": "^2.0.1",
    "@angular/forms": "^2.0.1",
    "@angular/platform-browser": "^2.0.1",
    "bootstrap": "^3.3.7",
    "rxjs": "^5.0.0-beta.12",
    "typescript": "^2.0.3",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "copyfiles": "^1.0.0"
